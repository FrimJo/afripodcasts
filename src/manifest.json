{
  "manifest_version": 1,
  "version": "1.0.0",
  "name": "Angular 2 Boilerplate",
  "short_name": "Boilerplate",
  "description": "A biolerplate for Angular 2 projects.",
  "default_locale": "se",
  "icons": [
    {
      "src": "/assets/icons/android-chrome-192x192.png",
